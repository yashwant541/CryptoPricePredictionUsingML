DECLARE @startRow INT = 0;
DECLARE @endRow INT = 10000;

WHILE @startRow < 1000000  -- Replace 1000000 with the total row count in your table
BEGIN
    WITH numbered_rows AS (
        SELECT *, ROW_NUMBER() OVER (ORDER BY some_column) AS row_num
        FROM your_table_name
    )
    SELECT *
    FROM numbered_rows
    WHERE row_num > @startRow AND row_num <= @endRow;

    SET @startRow = @endRow;
    SET @endRow = @endRow + 10000;
END;
