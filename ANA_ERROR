
//2024-01 to 2024-06
if 
(
[TO_CHAR(A.JOURNAL_DATE,'YYYY-MM-DD')] - #2024-01-31# < 0
AND
[TO_CHAR(A.POSTED_DATE,'YYYY-MM-DD')] - #2024-01-31# > 5
)

THEN 
[TO_CHAR(A.POSTED_DATE,'YYYY-MM-DD')] - #2024-01-31# 
ELSEIF
