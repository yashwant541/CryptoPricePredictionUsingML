from PyPDF2 import PdfReader, PdfWriter

def merge_pdfs(first_pdf, second_pdf, output_pdf):
    # Read the first PDF
    first_reader = PdfReader(first_pdf)
    # Read the second PDF
    second_reader = PdfReader(second_pdf)
    # Create a writer object
    writer = PdfWriter()

    # Add the first page from the first PDF
    writer.add_page(first_reader.pages[0])

    # Add all pages except the first from the second PDF
    for page in second_reader.pages[1:]:
        writer.add_page(page)

    # Write the output file
    with open(output_pdf, "wb") as output_file:
        writer.write(output_file)

# Example usage
first_pdf_path = "path/to/first.pdf"
second_pdf_path = "path/to/second.pdf"
output_pdf_path = "path/to/output.pdf"

merge_pdfs(first_pdf_path, second_pdf_path, output_pdf_path)
